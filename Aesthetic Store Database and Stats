CREATE TABLE Jax (id INTEGER PRIMARY KEY, item TEXT, quantity INTEGER, price INTEGER, aisle INTEGER);
INSERT into Jax VALUES (1, "Toner", 4, 10.00, 3);
INSERT into Jax VALUES (2, "Serum", 3,20.00, 2);
INSERT into Jax VALUES (3, "Moisturizer", 5, 15.00,1);
INSERT into Jax VALUES (4, "Primer", 5, 20.00,4);
INSERT into Jax VALUES (5, "Face Wash", 10, 25.00,5);

SELECT * FROM Jax;
SELECT * FROM Jax ORDER by price; 
SELECT * FROM Jax ORDER by quantity;
